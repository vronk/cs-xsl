<?xml version="1.0" encoding="UTF-8"?>
<project name="load-ant-props" xmlns:props="antlib:org.apache.ant.props">
    <dirname property="load-ant-props.jardir" file="${ant.file.load-ant-props}"/>
    <taskdef uri="antlib:org.apache.ant.props" resource="org/apache/ant/props/antlib.xml">
        <classpath>
            <pathelement location="${load-ant-props.jardir}/ant-props-1.0Alpha.jar"/>
        </classpath>
    </taskdef>
    <!-- Activate Props antlib -->
    <propertyhelper>
        <props:nested/>
    </propertyhelper>
</project>

